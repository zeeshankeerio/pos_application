{
    "name": "raheel_fabrics",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "dev:powershell": "powershell -ExecutionPolicy Bypass -Command \"next dev\"",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier --write .",
        "postinstall": "prisma generate && node generate-ledger-client.js",
        "seed": "npx ts-node --project prisma/tsconfig.seed.json prisma/seed.ts",
        "check:db": "node --experimental-modules check-db.js",
        "test:sales": "node --experimental-modules test-sales.js",
        "vercel-build": "prisma generate && prisma db push && node generate-ledger-client.js && prisma db push --schema=prisma/schema-ledger.prisma && next build",
        "generate:ledger": "node generate-ledger-client.js",
        "db:push:ledger": "prisma db push --schema=prisma/schema-ledger.prisma",
        "fix:ledger": "node fix-ledger-setup.js",
        "setup:ledger": "powershell -ExecutionPolicy Bypass -Command ./fix-ledger-setup.ps1"
    },
